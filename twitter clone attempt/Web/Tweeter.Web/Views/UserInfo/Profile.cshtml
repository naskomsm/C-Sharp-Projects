@model IEnumerable<TweetListingServiceModel>

@{
    ViewData["Title"] = "Profile Page";
}

<div class="row">
    <div class="col-sm-12">
        <div class="jumbotron">
            HEADER
        </div>
    </div>
</div>

<div class="row">
    <div class="col-sm-3">
        <div class="card">
            <div class="card-body">
                <p>profile</p>
            </div>
        </div>
    </div>
    <div class="col-sm-6">
        @foreach (var tweet in Model)
        {
            <div class="card">
                <div class="card-body">
                    <p>tweet</p>
                </div>
            </div>
        }
    </div>
    <div class="col-sm-3">
        <div class="card">
            <div class="card-body">
                <p>who to follow</p>
            </div>
        </div>
    </div>
</div>

<div class="row">
    <div class="col-sm-3 offset-9">
        <div class="card">
            <div class="card-body">
                <p>trends</p>
            </div>
        </div>
    </div>
</div>
